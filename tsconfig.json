{
    "compilerOptions": {
        "types": [
            "node",
            "jest"
        ],
        "module": "commonjs",
        "target": "ES2016",
        "esModuleInterop": true,
        "strict": true,
        "rootDir": "src",
        "outDir": "dist",
        "declaration": true,
        "sourceMap": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true
    },
    "exclude": ["dist/**/*"]
}
